    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="a, b, c" type="Integer" array="False" size=""/>
            <input variable="a"/>
            <input variable="b"/>
            <input variable="c"/>
            <if expression="a == b &amp;&amp; b == c">
                <then>
                    <output expression="&quot;triangulo equilatero&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="a=b || a=c || b=c">
