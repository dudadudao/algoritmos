                        </then>
                        <else>
                            <if expression="a&lt;b+c &amp;&amp; b&lt;a+c &amp;&amp; c&lt;a+b">
                                <then>
                                    <output expression="&quot;triangulo retangulo&quot;" newline="True"/>
                                </then>
                                <else>
                                    <output expression="&quot;n&#227;o pode ser triangulo&quot;" newline="True"/>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
            <output expression="" newline="True"/>
